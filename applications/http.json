{
    "name": "HTTP Web Server NGINX",
    "description": "A full HTTP web server. It will read from any directory but cannot handle .htaccess and php",
    "vars": ["path", "hostname"],
    "docker-compose": {
        "image": "kyma/docker-nginx",
        "volumes": ["$path:/var/www"],
        "labels": ["traefik.backend=Portainer", "traefik.frontend.rule=Host:$hostname", "traefik.docker.network=proxy", "traefik.port=80"],
        "restart": "always"

    }
}
